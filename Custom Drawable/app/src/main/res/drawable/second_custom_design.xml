<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="line">
            <stroke
                android:width="2dp"/>
        </shape>
    </item>
    <item>
        <rotate android:fromDegrees="45">
            <shape android:shape="line">
                <stroke android:width="2dp"/>
            </shape>
        </rotate>
    </item>
    <item>
        <rotate android:fromDegrees="90">
            <shape android:shape="line">
                <stroke android:width="2dp"/>
            </shape>
        </rotate>
    </item>
    <item>
        <rotate android:fromDegrees="135">
            <shape android:shape="line">
                <stroke android:width="2dp"/>
            </shape>
        </rotate>
    </item>
    <item>
        <rotate android:fromDegrees="22.5">
            <shape android:shape="line">
                <stroke android:width="2dp"/>
            </shape>
        </rotate>
    </item>
    <item>
        <shape
            android:shape="oval"
            android:width="20dp"
            android:height="20dp"
            android:radius="10dp"
            >
            <stroke android:width="2dp"/>
        </shape>
    </item>

</layer-list>